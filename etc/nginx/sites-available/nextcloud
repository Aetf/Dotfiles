server {
	listen 80;
	listen [::]:80;
	server_name files.unlimitedcodeworks.xyz next.unlimitedcodeworks.xyz;

	return 301 https://$server_name$request_uri;
}

server {
	listen 443 ssl http2;
	listen [::]:443 ssl http2;

	server_name files.unlimitedcodeworks.xyz next.unlimitedcodeworks.xyz;

	access_log /var/log/nginx/sites/nextcloud.access netdata;
	error_log /var/log/nginx/sites/nextcloud.error error;

    include snippets/cert/unlimitedcodeworks.xyz.conf;

    include sites-common/nextcloud;
}

server {
	listen 80;
	listen [::]:80;
	server_name files.unlimited-code.works next.unlimited-code.works;

	return 301 https://$server_name$request_uri;
}

server {
	listen 443 ssl http2;
	listen [::]:443 ssl http2;

	server_name files.unlimited-code.works next.unlimited-code.works;

	access_log /var/log/nginx/sites/nextcloud.access netdata;
	error_log /var/log/nginx/sites/nextcloud.error error;

    include snippets/cert/unlimited-code.works.conf;

    include sites-common/nextcloud;
}
