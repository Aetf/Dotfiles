server {
	listen 80;
	listen [::]:80;
	server_name jupyter.unlimited-code.works;

	return 301 https://$server_name$request_uri;
}

server {
	listen 443 ssl http2;
	listen [::]:443 ssl http2;

	server_name jupyter.unlimited-code.works;

	access_log /var/log/nginx/sites/jupyter.access netdata;
	error_log /var/log/nginx/sites/jupyter.error error;
	rewrite_log on;

    include snippets/cert/unlimited-code.works.conf;

    include sites-common/jupyter;
}

server {
	listen 80;
	listen [::]:80;
	server_name jupyter.unlimitedcodeworks.xyz;

	return 301 https://$server_name$request_uri;
}

server {
	listen 443 ssl http2;
	listen [::]:443 ssl http2;

	server_name jupyter.unlimitedcodeworks.xyz;

	return 301 https://jupyter.unlimited-code.works$request_uri;
}
