{
  "aria2": {
    "args": [
      "--allow-overwrite=true",
      "--continue=true",
      "--file-allocation=none",
      "--log-level=error",
      "--max-connection-per-server=15",
      "--max-concurrent-downloads=100",
      "--max-tries=10",
      "--max-file-not-found=10",
      "--min-split-size=2M",
      "--remote-time=true",
      "--timeout=5",
      "--always-resume=false",
      "--auto-file-renaming=false",
      "--check-integrity=true",
      "--conditional-get=true",
      "--show-console-readout=true",
      "--no-conf"
    ],
    "path": "/usr/bin/aria2c"
  },
  "pacman": {
    "config": "/etc/pacman.conf",
    "path": "/usr/bin/pacman"
  },
  "pacserve": {
    "server": null
  },
  "powerpill": {
    "select": true,
    "reflect databases": false
  },
  "reflector": {
    "args": [
      "--country",
      "US",
      "--sort",
      "rate",
      "--protocol",
      "rsync"
    ]
  },
  "rsync": {
    "args": [
      "--no-motd",
      "--progress"
    ],
    "db only": true,
    "path": "/usr/bin/rsync",
    "servers": [
      "rsync://mirror.umd.edu/archlinux/$repo/os/$arch",
      "rsync://ftp.osuosl.org/archlinux/$repo/os/$arch",
      "rsync://mirror.grig.io/archlinux/$repo/os/$arch",
      "rsync://mirror.wdc1.us.leaseweb.net/archlinux/$repo/os/$arch",
      "rsync://mirrors.rit.edu/archlinux/$repo/os/$arch",
      "rsync://arch.mirror.square-r00t.net/arch/$repo/os/$arch",
      "rsync://rsync.gtlib.gatech.edu/archlinux/$repo/os/$arch",
      "rsync://mirrors.aggregate.org/archlinux/$repo/os/$arch",
      "rsync://mirror.cs.pitt.edu/archlinux/$repo/os/$arch",
      "rsync://arch.mirror.constant.com/archlinux/$repo/os/$arch",
      "rsync://arlm.tyzoid.com/archlinux/$repo/os/$arch",
      "rsync://mirror.epiphyte.network/archlinux/$repo/os/$arch",
      "rsync://mirror.es.its.nyu.edu/archlinux/$repo/os/$arch",
      "rsync://distro.ibiblio.org/archlinux/$repo/os/$arch",
      "rsync://mirror.dal10.us.leaseweb.net/archlinux/$repo/os/$arch",
      "rsync://mirrors.liquidweb.com/archlinux/$repo/os/$arch",
      "rsync://mirror.cc.columbia.edu/archlinux/$repo/os/$arch",
      "rsync://mirrors.sonic.net/archlinux/$repo/os/$arch",
      "rsync://mirror.math.princeton.edu/pub/archlinux/$repo/os/$arch",
      "rsync://mirrors.cat.pdx.edu/archlinux/$repo/os/$arch",
      "rsync://mirror.as65535.net/archlinux/$repo/os/$arch",
      "rsync://mirror.sfo12.us.leaseweb.net/archlinux/$repo/os/$arch",
      "rsync://mirror.lty.me/archlinux/$repo/os/$arch",
      "rsync://mirrors.advancedhosters.com/archlinux/$repo/os/$arch",
      "rsync://mirrors.ocf.berkeley.edu/archlinux/$repo/os/$arch"
    ]
  }
}
